{
  "name": "supascribe-notes-mcp",
  "version": "0.1.0",
  "description": "MCP server that writes index cards to Supabase, deployed on Cloud Run",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node --env-file=.env dist/index.js",
    "dev": "node --env-file=.env --import tsx/esm --watch src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "mcp",
    "supabase",
    "cloud-run",
    "index-cards"
  ],
  "license": "PolyForm-Shield-1.0.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@supabase/supabase-js": "^2.49.0",
    "cors": "^2.8.6",
    "express": "^5.1.0",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "zod": "^3.25.0",
    "zod-to-json-schema": "^3.25.1"
  },
  "devDependencies": {
    "@checkmarkdevtools/commitlint-plugin-rai": "^0.1.6",
    "@commitlint/cli": "^19.7.0",
    "@commitlint/config-conventional": "^19.7.0",
    "@eslint/js": "^9.21.0",
    "@secretlint/secretlint-rule-preset-recommend": "^11.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.0",
    "@types/pino": "^7.0.4",
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "lefthook": "^1.11.0",
    "prettier": "^3.5.0",
    "secretlint": "^11.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.24.0",
    "vitest": "^3.0.0"
  }
}
