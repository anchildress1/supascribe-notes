{
  "name": "supascribe-notes-mcp",
  "version": "0.1.0",
  "description": "MCP server that writes index cards to Supabase, deployed on Cloud Run",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "eslint --fix . && prettier --write .",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "mcp",
    "supabase",
    "cloud-run",
    "index-cards"
  ],
  "license": "PolyForm-Shield-1.0.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@supabase/supabase-js": "^2.49.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@checkmarkdevtools/commitlint-plugin-rai": "^0.1.6",
    "@commitlint/cli": "^19.7.0",
    "@commitlint/config-conventional": "^19.7.0",
    "@eslint/js": "^9.21.0",
    "@secretlint/secretlint-rule-preset-recommend": "^11.3.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.0",
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.21.0",
    "lefthook": "^1.11.0",
    "prettier": "^3.5.0",
    "secretlint": "^11.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.24.0",
    "vitest": "^3.0.0"
  }
}
